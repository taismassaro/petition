<main id="supporters">
    {{>nav}}
    
    <p id="header">make teleportation happen <span>supporters</span></p>
{{#signers}}
{{#if url}}
    {{#unless @last}}
    <div><span>{{incremented @index}}</span>&emsp;<a href={{url}} target="_blank">{{first}} {{last}}</a>{{#if age}}&ensp;|&ensp;{{age}}{{/if}}{{#if city}}&ensp;|&ensp;<a href="/supporters/{{city}}">{{city}}</a>{{/if}}</div>
