    <div><span>{{incremented @index}}</span>&emsp;{{first}} {{last}}{{#if age}}&ensp;|&ensp;{{age}}{{/if}}{{#if city}}&ensp;|&ensp;{{city}}{{/if}}</div>
    {{/unless}}
    {{#if @last}}
    <div><span>{{incremented @index}}</span>&emsp;<strong>{{first}} {{last}}</strong>{{#if age}}&ensp;|&ensp;{{age}}{{/if}}{{#if city}}&ensp;|&ensp;{{city}}{{/if}}</div>
    {{/if}}
{{/if}}
{{/signers}}

